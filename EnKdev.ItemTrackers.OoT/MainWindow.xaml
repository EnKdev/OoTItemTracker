        MouseDown="MainWindow_OnMouseDown" ResizeMode="NoResize" Topmost="True"
        Deactivated="MainWindow_OnDeactivated">
    <Window.DataContext>
        <local:MainWindowViewModel/>
    </Window.DataContext>
